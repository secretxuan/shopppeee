# 服务配置
APP_NAME=Shoppee
APP_ENV=development
APP_PORT=8080
APP_DEBUG=true

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=shoppee
DB_SSLMODE=disable
DB_TIMEZONE=Asia/Shanghai

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# JWT配置
JWT_SECRET=your-super-secret-key-change-in-production
JWT_EXPIRE_HOURS=24

# 日志配置
LOG_LEVEL=debug
LOG_FILE_PATH=./logs/app.log

# CORS配置
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080
